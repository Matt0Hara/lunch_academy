<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
    <%= f.email_field :email, placeholder:"Email", autofocus: false, class: "cubeInput" %>
    <%= f.password_field :password, placeholder: "Password", class: "cubeInput" %>
    <%= f.submit "Sign in", class: "button tiny cubeButton"%><br/>
  <% if devise_mapping.rememberable? -%>
  <% end -%>
<% end %>
<%= render "devise/shared/specialLinks" %>
